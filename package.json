{
  "name": "gitbar",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "codegen": "graphql-codegen --config codegen.ts",
    "format": "eslint --ext=js,jsx,ts,tsx --fix src && prettier --write src ",
    "lint:eslint": "eslint --ext=js,jsx,ts,tsx --max-warnings=0 src",
    "lint:prettier": "prettier --check --loglevel=warn src",
    "lint:tsc": "tsc --noEmit",
    "lint": "concurrently --kill-others-on-fail \"npm:lint:*\"",
    "mac": "react-native run-macos --scheme=GitBar",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "start": "expo start",
    "test": "jest",
    "changelog": "source .env && git-cliff -o --github-repo 'TLaMars/gitbar' --github-token $GITHUB_TOKEN",
    "bump:build_number": "bundle exec fastlane mac bump_version",
    "version:bump": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "@apollo/client": "^3.12.3",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.0.0",
    "@sentry/react-native": "6.0.0",
    "@tanstack/react-query": "^5.62.3",
    "expo": "52.0.6",
    "expo-font": "~13.0.1",
    "expo-modules-core": "^2.0.2",
    "graphql": "^16.8.1",
    "js-base64": "^3.7.7",
    "react": "18.3.1",
    "react-intl": "^6.6.3",
    "react-native": "0.76.5",
    "react-native-macos": "^0.76.5",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.10.1",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-native-community/cli": "^15.1.3",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitlint": "^19.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-triple": "1.1.6",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "expo-crypto": "~14.0.1",
    "git-cliff": "^2.5.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "prettier": "eslint-config-triple/.prettierrc",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "!(.*/**)*.(ts|tsx)": [
      "eslint --max-warnings=0",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "!(.*/**)*.(js|cjs|mjs|jsx)": [
      "eslint --max-warnings=0"
    ]
  }
}
